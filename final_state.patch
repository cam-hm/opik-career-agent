+};
+
+const staggerContainer = {
+    hidden: { opacity: 0 },
+    visible: { opacity: 1, transition: { staggerChildren: 0.1 } }
+};
+
+export default function FAQ() {
+    const { t } = useLanguage();
+
+    const faqKeys = ["1", "2", "3", "4", "5"];
