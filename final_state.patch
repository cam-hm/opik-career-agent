+    const fetchApplications = async () => {
+        try {
+            const res = await fetchWithAuth(ENDPOINTS.APPLICATION.LIST, {}, getToken);
+            if (res.ok) {
+                const data = await res.json();
+                setApplications(data);
+            }
+        } catch (error) {
+            console.error("Failed to fetch applications", error);
+        } finally {
+            setLoading(false);
+        }
+    };
