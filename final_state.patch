+    echo=settings.DEBUG
+)
+
+# Session factory
+AsyncSessionLocal = sessionmaker(
+    engine, 
+    class_=AsyncSession, 
+    expire_on_commit=False
+)
+
+
+async def get_db():
+    """
+    Dependency for FastAPI endpoints.
+    
+    Usage:
+        @router.get("/")
