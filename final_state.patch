+                                    <li key={item.href}>
+                                        <Link
+                                            href={item.href}
+                                            className={`flex items-center gap-3 px-3 py-2 rounded-lg transition-colors ${isActive
+                                                ? "bg-gray-50 dark:bg-gray-500/10 text-[#424874] dark:text-[#A6B1E1]"
