+                    </div>
+                    {/* Title */}
+                    <div className="flex-1 min-w-0">
+                        <h1 className="text-xl font-semibold text-gray-900 dark:text-white">{application.job_role}</h1>
+                        <p className="text-sm text-gray-500 dark:text-gray-400">{t('applicationView.title')}</p>
