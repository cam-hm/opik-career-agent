+                        <FAQItem
+                            key={key}
+                            question={t(`faq.items.${key}.q`)}
+                            answer={t(`faq.items.${key}.a`)}
+                        />
+                    ))}
+                </motion.div>
+            </div>
+        </section>
+    );
+}
+
+function FAQItem({ question, answer }: { question: string; answer: string }) {
+    const [isOpen, setIsOpen] = useState(false);
+
